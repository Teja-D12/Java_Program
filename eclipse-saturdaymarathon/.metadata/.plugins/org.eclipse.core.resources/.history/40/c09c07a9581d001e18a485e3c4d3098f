package Temperatureconverter.Temperatureconverter;
import java.util.*;
public class TemperatureConverter {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter the value of Fareinheat");
		double a=scan.nextDouble();
		double res=Fahrentocelsius(a);
		System.out.println("temperature converted to celsius is:"+ res);
		
		
	}
	public static double Fahrentocelsius(double a)
	{
		double c=(a-32)*5/9;
		return c;
	}
}

