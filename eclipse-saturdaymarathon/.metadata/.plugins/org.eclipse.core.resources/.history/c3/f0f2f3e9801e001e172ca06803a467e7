package average.average;
import java.util.*;
public class Average {
	public static void main(String[] args) {

	Scanner scan=new Scanner(System.in);
	System.out.println("Enter 8 numbers");
	double num1=scan.nextDouble();
	double num2=scan.nextDouble();
	double num3=scan.nextDouble();
	double num4=scan.nextDouble();
	double num5=scan.nextDouble();
	double num6=scan.nextDouble();
	double num7=scan.nextDouble();
	double num8=scan.nextDouble();
	double res1=Average(num1,num2,num3,num4,num5,num6,num7,num8);
	System.out.println(res1);
			
	
}
public static double Average(double num1,double num2,double num3,double num4,double num5,double num6,double num7,double num8)
{
	double res=num1+num2+num3+num4+num5+num6+num7+num8;
	double res1=res/8;
	return res1;
}
}


