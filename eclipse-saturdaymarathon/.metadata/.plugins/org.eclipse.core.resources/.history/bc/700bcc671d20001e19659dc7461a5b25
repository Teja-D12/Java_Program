package Patternprinting;

public class Pattern5 {
public static void main(String[] args) {
	int i;
	int j;
	int n=5;
	
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=n;j++)	
		{
			if(i==1 || j==1 || i==5 || j==5)
			{
			System.out.println(i+" ");	
			}
			else
			{
			System.out.println(" ");	
			}
		}
		
	}
}
}
